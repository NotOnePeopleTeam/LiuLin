  background-color: #FFEFEF;
  width: 260rpx;
  border-radius: 18rpx;
  height: 18rpx;
  position: relative;
}

.listt .item .text .progress .bg-reds {
  width: 0;
  height: 100%;
  transition: width 0.6s ease;
  background: linear-gradient(90deg, rgba(233, 51, 35, 1) 0%, rgba(255, 137, 51, 1) 100%);
}

.listt .item .text .progress .piece {
  position: absolute;
  left: 8%;
  transform: translate(0%, -50%);
  top: 49%;
  font-size: 16rpx;
  color: #FFB9B9;
}

.listt .item .grab {
  background-color: red;
  font-size: 28rpx;
  color: #fff;
  width: 150rpx;
  height: 54rpx;
  border-radius: 27rpx;
  text-align: center;
  line-height: 54rpx;
  position: absolute;
  right: 30rpx;
  bottom: 30rpx;
}

.saleBox {
  width: 100%;
  height: 230rpx;
  background: rgba(233, 51, 35, 1);
  border-radius: 0 0 50rpx 50rpx;
}

/* 砍价 */
.listss {
  background-color: #fff;
  /* border: 6rpx solid #fc8b42; */
  border-radius: 30rpx;
  margin: 35rpx 30rpx 66rpx 30rpx;
  padding: 10rpx 24rpx;
}

.listss .item {
  border-bottom: 1rpx solid #eee;
  position: relative;
  height: 223rpx;
}

.listss .item .pictrue {
  width: 160rpx;
  height: 160rpx;
}

.listss .item .pictrue image {
  width: 100%;
  height: 100%;
  border-radius: 6rpx;
}

.listss .item .text {
  width: 450rpx;
  font-size: 30rpx;
  color: #282828;
  height: 160rpx;
}

.listss .item .text .num {
  font-size: 26rpx;
  color: #999;
}

.listss .item .text .num .iconfont {
  font-size: 35rpx;
  margin-right: 7rpx;
}

.listss .item .text .money {
  font-size: 24rpx;
  font-weight: bold;
}

.listss .item .text .money .price {
  font-size: 32rpx;
}

.listss .item .text .line1 {
  width: 450rpx;
}

.listss .item .cutBnt {
  position: absolute;
  width: 180rpx;
  height: 50rpx;
  border-radius: 50rpx;
  font-size: 24rpx;
  color: #fff;
  text-align: center;
  line-height: 46rpx;
  right: 6rpx;
  box-shadow: 0 7rpx 0 #f8c1bd;
  background-color: red;
  top: 120rpx;
}

.listss .item .cutBnt .iconfont {
  margin-right: 8rpx;
  font-size: 30rpx;
}

.listss .load {
  font-size: 24rpx;
  height: 85rpx;
  text-align: center;
  line-height: 85rpx;
}

.kefu {
  color: #7D7D7D;
  font-size: 26rpx;
  display: block;
}