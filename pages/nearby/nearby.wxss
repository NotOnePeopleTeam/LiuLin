/* pages/nearby/nearby.wxss */
.page {
  position: relative;
}

.titleing image {
  width: 100%;
  filter: alpha(opacity=0);
  opacity: .9;
}

.nearname {
  position: absolute;
  top: 390rpx;
  width: 100%;
  text-align: center;
  margin: auto;
  font-weight: bold;
  font-size: 32rpx;
  height: 100rpx;
  line-height: 100rpx;
  background-color: #fff;
 
 
}


.dianlist {
  background-color: #fff;
  display: flex;
  align-items: center;
  justify-content: space-around;
  width: 100%;
  height: 150rpx;
  margin-top: 20rpx;
}
.tit{
  font-size: 30rpx;
  color: #333333;
  font-weight: bold;
}
.address{
  color: #999999;
  font-size: 24rpx;
}
.dianlist image {
  width: 30rpx;
  height: 30rpx;
}